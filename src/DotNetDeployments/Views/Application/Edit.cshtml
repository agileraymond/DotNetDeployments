@model Amazon.CodeDeploy.Model.GetApplicationResponse

@{
    ViewData["Title"] = "Edit Application";
}

<h2>Edit Application</h2>
<form asp-controller="Application" asp-action="Edit" method="post" role="form">
    <div class="form-group">
        <label class="control-label">App Name</label>
        <div class="">
            <input asp-for="Application.ApplicationName" class="form-control" />
        </div>
          
    </div>
    <div class="form-group">
        <div class="">
            <input type="submit" class="btn btn-default" value="Update" />
        </div>
    </div>
</form>
